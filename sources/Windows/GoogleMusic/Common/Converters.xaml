<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="using:OutcoldSolutions.GoogleMusic.Converters">

    <converters:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
    <converters:BooleanToVisibilityConverter x:Key="BooleanToInvisibilityConverter" Invert="True" />
    <converters:InvertBooleanConverter x:Key="InvertBooleanConverter" />
    <converters:ValueToVisibilityConverter x:Key="ValueToVisibilityConverter" />
    <converters:ValueToVisibilityConverter x:Key="ValueToInvisibilityConverter" Invert="True"/>
    <converters:StringFormatConverter x:Key="StringFormatConverter"/>
    <converters:DoubleToTimeConverter x:Key="DoubleToTimeConverter"/>
    <converters:NullToVisibilityConverter x:Key="NullToVisibilityConverter"/>
    <converters:NullToVisibilityConverter x:Key="NullToInvisibilityConverter" Invert="True"/>
    <converters:AlbumArtUrlToImageConverter x:Key="AlbumArtUrlToImageConverter" />

</ResourceDictionary>
